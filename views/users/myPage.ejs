<table>
    <tr>
        <th>CATEGORY</th>
    </tr>
    <tr>
        <th>CODE</th>
        <td><%= cart.productCode %></td>
    </tr>
    <tr>
        <th>NAME</th>
        <td></td>
    </tr>
    <tr>
        <th>PRICE</th>
        <td></td>
    </tr>
</table>
<% } %>